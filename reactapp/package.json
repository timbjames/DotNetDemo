    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.19",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.16",
    "@types/react-redux-toastr": "^7.6.0",
    "axios": "^0.21.1",
    "bootstrap": "^5.1.0",
    "node-sass": "^5.0.0",
